<Page x:Class="Something.GameSelect"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Something"
      mc:Ignorable="d" MouseMove="mouseColorEffect"
      d:DesignWidth="420" d:DesignHeight="450"
      Title="GameSelect">

    <Grid x:Name="GameSelectScreen">
        <TextBlock Text="Choose a Story" VerticalAlignment="Top" FontSize="18" HorizontalAlignment="Center" Margin="0,10,0,0"/>
        <Rectangle x:Name="rect1" Fill="Black" Margin="0,0,0,46" VerticalAlignment="Bottom" Width="330" Height="360" Panel.ZIndex="0"/>
        <StackPanel x:Name="SelectPanel" VerticalAlignment="Center" Width="250" MaxHeight="300"/>
        <Button Content="Go Back" HorizontalAlignment="Center" VerticalAlignment="Bottom" Background="Black" BorderBrush="{x:Null}" Foreground="White" Width="50" Height="21" Margin="0,0,0,27"/>

        <Rectangle x:Name="centerPoint" Height="1" Width="1" Panel.ZIndex="-1"/>
    </Grid>

    <Page.Resources>
        
    </Page.Resources>
</Page>
