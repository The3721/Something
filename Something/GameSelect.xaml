<Page x:Class="Something.GameSelect"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Something"
      mc:Ignorable="d" MouseMove="mouseColorEffect"
      d:DesignHeight="420" d:DesignWidth="450"
      Title="GameSelect">

    <Grid x:Name="GameSelectScreen" Background="#FFBE3232">
        <StackPanel Background="#FF58C220" HorizontalAlignment="Center" VerticalAlignment="Center" Width="300" Height="100" >
            <Button x:Name="game1" />

        </StackPanel>


        <Rectangle x:Name="centerPoint" Height="1" Width="1" Panel.ZIndex="-1"/>
    </Grid>

    <Page.Resources>
        
    </Page.Resources>
</Page>
